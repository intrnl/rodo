  "peerDependencies": {
    "vite": "^1.0.0-rc.4"
  },
  "dependencies": {
    "@intrnl/rodo": "workspace:^0.3.0",
    "upath": "^1.2.0"
  },
  "devDependencies": {
		"@intrnl/bundl": "^0.1.8",
    "rollup": "^2.26.10",
    "vite": "^1.0.0-rc.4"
