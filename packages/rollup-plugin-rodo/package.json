{
  "name": "@intrnl/rollup-plugin-rodo",
  "description": "Rollup plugin for simple filesystem router",
  "version": "0.2.1",
  "license": "MIT",
  "author": "intrnl",
  "repository": {
    "url": "https://github.com/intrnl/rodo",
    "directory": "packages/rollup-plugin-rodo"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "build": "tsc -m commonjs --outDir dist/cjs/ & tsc -m esnext --outDir dist/esm/"
  },
  "peerDependencies": {
    "rollup": "^2.26.3"
  },
  "dependencies": {
    "@intrnl/rodo": "workspace:^0.2.1",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "rollup": "^2.26.5"
  }
}
