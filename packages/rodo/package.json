		"types/",
    "README.md"
  ],
  "scripts": {
		"build": "pnpm run build:lib & pnpm run build:types",
		"build:lib": "bundl",
		"build:types": "tsc",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "upath": "^1.2.0"
  },
  "devDependencies": {
    "@intrnl/bundl": "^0.1.8",
    "@types/node": "^14.6.4"
  }
}
