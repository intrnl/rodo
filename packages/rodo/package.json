{
  "name": "@intrnl/rodo",
  "description": "Simple filesystem router",
  "version": "0.2.1",
  "license": "MIT",
  "author": "intrnl",
  "repository": {
    "url": "https://github.com/intrnl/rodo",
    "directory": "packages/rodo"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
