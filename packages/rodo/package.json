{
  "name": "@intrnl/rodo",
  "description": "Simple filesystem router",
  "version": "0.3.0",
  "license": "MIT",
  "author": "intrnl",
  "repository": {
    "url": "https://github.com/intrnl/rodo",
    "directory": "packages/rodo"
	},
	"source": "lib/index.ts",
  "main": "dist/rodo.js",
	"module": "dist/rodo.mjs",
	"types": "types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/rodo.js",
      "import": "./dist/rodo.mjs"
